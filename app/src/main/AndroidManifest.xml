<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cn.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ui.auth.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.auth.RegisterActivity"
            android:label="@string/title_register" />

        <activity
            android:name=".ui.main.MainActivity"
            android:label="@string/title_main" />

        <activity
            android:name=".ui.shop.ShopDetailActivity"
            android:label="@string/title_shop_detail" />

        <activity
            android:name=".ui.shop.ShopListActivity"
            android:label="@string/title_shop_list" />

        <activity
            android:name=".ui.food.FoodDetailActivity"
            android:label="@string/title_food_detail" />

        <activity
            android:name=".ui.cart.CartActivity"
            android:label="@string/title_cart" />

        <activity
            android:name=".ui.order.CheckoutActivity"
            android:label="@string/checkout_title" />

        <activity
            android:name=".ui.order.OrderConfirmActivity"
            android:label="@string/title_order_confirm" />

        <activity
            android:name=".ui.search.ProductWebSearchActivity"
            android:label="@string/title_product_web_search" />

        <activity
            android:name=".ui.entry.QuickEntryActivity"
            android:label="@string/quick_entry_title" />

        <activity
            android:name=".ui.user.UserProfileActivity"
            android:label="@string/title_user_profile" />
    </application>

</manifest>
